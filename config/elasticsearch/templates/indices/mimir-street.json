{
  "elasticsearch": {
    "index_patterns": ["{{ROOT}}_street*"],
    "template": {
      "mappings": {
        "properties": {
          "type": {
            "type": "constant_keyword",
            "value": "street"
          }
        }
      }
    },
    "composed_of": ["mimir-base"],
    "version": 3,
    "priority": 10
  }
}
