{
  "elasticsearch": {
    "index_patterns": ["munin_addr*"],
    "template": {
      "settings": {
        "analysis": {
          "analyzer": {
            "word":{
              "filter":[
                "lowercase",
                "asciifolding"
              ],
              "type":"custom",
              "tokenizer":"standard"
            }
          }
        }
      },
      "mappings": {
        "properties": {
          "house_number": {
            "type": "text",
            "analyzer": "word"
          },
          "street": {
            "enabled": false
          }
        }
      }
    },
    "composed_of": ["mimir-base"],
    "version": 3,
    "priority": 10
  }
}
