{
	"elasticsearch": {
		"index_patterns": ["munin_stop*"],
		"template": {
			"mappings": {
				"properties": {
          "comments": {
            "enabled": false
          },
          "commercial_modes": {
            "enabled": false
          },
          "coverages": {
            "enabled": false
          },
          "feed_publishers": {
            "enabled": false
          },
